<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="600.0" prefWidth="150.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controlador.admi.SideController">
    <children>
        <TitledPane fx:id="side" collapsible="false" text="OPCIONES">
            <font>
                <Font name="System Italic" size="17.0" />
            </font>
            <content>
                <Accordion>
                    <panes>
                        <TitledPane animated="false" text="Almacen">
                            <content>
                                <VBox>
                                    <children>
                                        <Button defaultButton="true" mnemonicParsing="false" onAction="#m_productos" prefHeight="17.0" prefWidth="117.0" text="Productos" />
                                        <Button defaultButton="true" mnemonicParsing="false" onAction="#m_categoria" prefHeight="27.0" prefWidth="117.0" text="Categoria" />
                                        <Button blendMode="GREEN" cancelButton="true" mnemonicParsing="false" prefHeight="27.0" prefWidth="117.0" text="Button" />
                                    </children>
                                </VBox>
                            </content>
                        </TitledPane>
                  <TitledPane animated="false" collapsible="false" expanded="false">
                     <graphic>
                        <Button defaultButton="true" mnemonicParsing="false" onAction="#ventas" prefHeight="37.0" prefWidth="122.0">
                           <graphic>
                              <Label text="Ventas">
                                 <graphic>
                                    <ImageView fitHeight="63.0" fitWidth="32.0" pickOnBounds="true" preserveRatio="true">
                                       <image>
                                          <Image url="@../../recursos/GG_opt.png" />
                                       </image>
                                    </ImageView>
                                 </graphic>
                              </Label>
                           </graphic>
                        </Button>
                     </graphic>
                  </TitledPane>
                    </panes>
                </Accordion>
            </content>
        </TitledPane>
    </children>
</VBox>
